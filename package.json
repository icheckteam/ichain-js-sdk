{
  "name": "ichain-js-sdk",
  "version": "1.0.0",
  "description": "Ichain Javascript SDK",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/mocha ./test  && npm run test:typings",
    "test:unit": "./node_modules/.bin/mocha ./test/unit",
    "test:integration": "./node_modules/.bin/mocha ./test/integration",
    "test:typings": "./node_modules/.bin/tsc",
    "build:dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --mode development",
    "build:prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --mode production",
    "watch:test": "./node_modules/.bin/mocha ./test/unit --bail --watch --reporter=min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icheckteam/ichain-js-sdk.git"
  },
  "keywords": [
    "ichain"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "author": "thangn.1411@gmail.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/icheckteam/ichain-js-sdk/issues"
  },
  "homepage": "https://github.com/icheckteam/ichain-js-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@types/loglevel": "^1.5.3",
    "babel-loader": "^8.0.0-beta.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.6",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "typescript": "^2.9.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bignumber.js": "^7.2.1",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.1",
    "crypto-js": "^3.1.9-1",
    "elliptic": "^6.4.0",
    "loglevel": "^1.6.1",
    "loglevel-plugin-prefix": "^0.8.3",
